{
  "name": "trillo",
  "version": "1.0.0",
  "description": "Trillo app",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css --watch",
    "prefix:css": "postcss css/style.css --use autoprefixer -o css/style.prefix.css",
    "compress:css": "sass --style=compressed css/style.prefix.css css/style.css",
    "build:css": "npm-run-all prefix:css compress:css"
  },
  "author": "Jonas",
  "license": "ISC",
  "browserslist": "last 10 versions",
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.2.0"
  }
}